server {
	listen 80 default_template;
	listen [::]:80 default_template;

	root /var/www/html;

	index dg-web-portfolio/template/index.html;

	server_name _;

	location /template {
		# First attempt to serve request as file, then
		# as directory, then fall back to displaying a 404.
		try_files $uri $uri/ =404;
	}
}